<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE sesx>
<sesx version="1.7">
<session appBuild="12.1.3.10" appVersion="12.1" audioChannelType="stereo" bitDepth="32" duration="{{duration*zoom}}" sampleRate="{{samplerate}}">
<tracks>{% for track in tracks %}
<audioTrack automationLaneOpenState="false" id="{{track.idx+10001}}" index="{{track.idx+1}}" select="false" visible="true">
<trackParameters trackHeight="150" trackMinimized="false">
<name>{{track.name}}</name>
</trackParameters>
<trackAudioParameters audioChannelType="stereo" automationMode="1" monitoring="false" recordArmed="false" solo="false" soloSafe="false">
<trackOutput outputID="10000" type="trackID"/>
</trackAudioParameters>{% for line in track.lines %}
<audioClip clipAutoCrossfade="true" crossFadeHeadClipID="-1" crossFadeTailClipID="-1" endPoint="{{line.ext_time*zoom}}" fileID="{{line.file}}" hue="-1" id="{{line.idx}}" lockedInTime="false" looped="false" name="{{line.title}}" offline="false" select="false" sourceInPoint="{{line.sop*zoom}}" sourceOutPoint="{{line.eop*zoom}}" startPoint="{{line.now_time*zoom}}" zOrder="0">
</audioClip>{% endfor %}
</audioTrack>{% endfor %}
<masterTrack automationLaneOpenState="false" id="10000" index="0" select="false" visible="true">
<trackParameters trackHeight="150" trackHue="-1" trackMinimized="false">
<name>MAIN</name>
</trackParameters>
<trackAudioParameters audioChannelType="stereo" automationMode="1" monitoring="false" recordArmed="false" solo="false" soloSafe="true">
<trackOutput outputID="1" type="hardwareOutput"/>
<trackInput inputID="-1"/>
</trackAudioParameters>
</masterTrack>
</tracks>
<sessionState ctiPosition="0" smpteStart="0">
<selectionState selectionDuration="0" selectionStart="0"/>
<mixingOptionState defaultPanModeLogarithmic="false" panPower="-3"/>
</sessionState>
</session>
<files>{% for file in files %}
<file id="{{file.idx}}" mediaHandler="{{file.handler}}" relativePath="{{file.path}}"/>{% endfor %}
</files>
</sesx>
